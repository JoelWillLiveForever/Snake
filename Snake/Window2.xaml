<Window x:Class="Snake.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Snake"
        mc:Ignorable="d"
        Title="Snake The Game" Height="450" Width="800" Icon="Resources/Snake.ico" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Resources/Snake_Background2.jpg"/>
        </Grid.Background>
        <ComboBox HorizontalAlignment="Left" Margin="10,194,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="ComboBox_SelectionChanged" FontFamily="/Snake;component/Resources/#NFS font" FontSize="20" Background="Black" BorderBrush="White" Foreground="White"/>
        <Button Content="BACK" HorizontalAlignment="Left" Margin="10,228,0,0" VerticalAlignment="Top" Width="120" Height="42" FontFamily="/Snake;component/Resources/#NFS font" Background="Black" BorderBrush="White" Foreground="White" Click="Button_Click"/>

    </Grid>
</Window>
